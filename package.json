{
    "name": "thingsdb-py",
    "displayName": "Python String ThingsDB",
    "description": "Highlight ThingsDB code in python multiline strings",
    "version": "0.0.2",
    "publisher": "transceptor-technology",
    "engines": {
      "vscode": "^1.41.0"
    },
    "galleryBanner": {
      "color": "#dedede",
      "theme": "light"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/thingsdb/vscode-thingsdb-py"
    },
    "bugs": {
      "url": "https://github.com/thingsdb/vscode-thingsdb-py/issues",
      "email": "jeroen@transceptor.technology"
    },
    "icon": "logo.png",
    "license": "SEE LICENSE IN LICENSE FILE",
    "categories": ["Programming Languages"],
    "keywords": ["python", "thingsdb", "thingsdb-py"],
    "contributes": {
      "languages": [
        {
          "id": "python",
          "aliases": [
            "python"
          ],
          "extensions": [
            ".py",
            ".py3"
          ]
        }
      ],
      "grammars": [
        {
          "injectTo": [
            "source.python"
          ],
          "scopeName": "inline.python-thingsdb",
          "path": "./syntaxes/python-inline-ti.json",
          "embeddedLanguages": {
            "meta.embedded.inline.thingsdb": "thingsdb"
          }
        }
      ]
    }
  }
